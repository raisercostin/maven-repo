  <developers>
    <developer>
      <name>Costin Grigore</name>
      <email>raisercostin@gmail.com</email>
      <organization>raisercostin</organization>
      <organizationUrl>http://raisercostin.org</organizationUrl>
    </developer>
  </developers>
  <scm>
    <connection>${scm.connection.url}</connection>
    <developerConnection>${scm.connection.url}</developerConnection>
    <url>${git.url}</url>
    <tag>jcrawler-0.3</tag>
  </scm>
  <issueManagement>
    <system>Issues</system>
    <url>${issues.url}</url>
  </issueManagement>
  <distributionManagement>
    <repository>
      <id>dist-release-${dist.id}</id>
      <url>${dist.release.url}</url>
    </repository>
    <snapshotRepository>
      <uniqueVersion>false</uniqueVersion>
      <id>dist-snapshot-${dist.id}</id>
      <url>${dist.snapshot.url}</url>
    </snapshotRepository>
    <!--
    <site>
      <id>dist-site-${dist.id}</id>
      <url>${dist.site.url}</url>
    </site>
     -->
  </distributionManagement>
  <properties>
    <createChecksum>true</createChecksum>
    <deployment.scope>compile</deployment.scope>
    <dist.id>github-raisercostin</dist.id>
    <dist.release.url>file://${project.build.directory}/myArtifacts</dist.release.url>
    <dist.site.url>file://${project.build.directory}/mySitess</dist.site.url>
    <dist.snapshot.url>file://${project.build.directory}/myArtifacts</dist.snapshot.url>
    <dist.url>${git.url}</dist.url>
    <!--<docker.image.prefix>registry.gitlab.com/dcsi/all/docker/custom</docker.image.prefix> -->
    <experimentalCoroutines>enable</experimentalCoroutines>
    <git.host>github.com</git.host>
    <git.hostGropRep>${git.host}/${git.userOrGroup}/${git.repo}</git.hostGropRep>
    <git.repo>${project.artifactId}</git.repo>
    <git.url>https://${git.host}/${git.userOrGroup}/${git.repo}</git.url>
    <git.userOrGroup>raisercostin</git.userOrGroup>
    <issues.url>${git.url}/issues</issues.url>
    <jackson-bom.version>2.15.3</jackson-bom.version>
    <jackson.version>2.12.5</jackson.version>
    <lombok.version>1.18.32</lombok.version>
    <m2e.apt.activation>jdt_apt</m2e.apt.activation>
    <mainClass>org.raisercostin.jcrawler.JCrawler</mainClass>
    <maven.build.timestamp.format>yyyyMMdd'T'HHmmss</maven.build.timestamp.format>
    <maven.test.skip>false</maven.test.skip>
    <myJavaVersion>17</myJavaVersion>
    <org>raisercostin</org>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <resource.delimiter>@</resource.delimiter>
    <scm.connection.url>scm:git:${git.url}.git</scm.connection.url>
    <skip.tests>true</skip.tests>
    <sonar.host.url>https://sonarcloud.io</sonar.host.url>
    <sonar.organization>${org}</sonar.organization>
    <springboot.version>3.1.5</springboot.version>
    <timestamp>${maven.build.timestamp}</timestamp>
  </properties>
  <dependencies>
    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
    </dependency>
    <dependency>
